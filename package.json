{
  "name": "@suthio/redash-mcp",
  "version": "0.0.8",
  "description": "MCP server for Redash integration",
  "type": "module",
  "bin": {
    "redash-mcp": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc && chmod +x dist/index.js dist/cli.js",
    "start": "node dist/cli.js",
    "dev": "ts-node src/index.ts",
    "test": "jest --passWithNoTests",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "inspector:headless": "BROWSER=none npx @modelcontextprotocol/inspector node dist/index.js",
    "e2e:test": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "redash",
    "mcp",
    "ai",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.1.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
